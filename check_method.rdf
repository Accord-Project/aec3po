  <owl:Class rdf:about="NumericalCheckMethod">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Numerical Check Method</rdfs:label>
    <rdfs:comment xml:lang="en">Numerical check method refers to a type of check that involves assessing and verifying numerical data or values for accuracy and adherence to specific criteria or standards. This method typically includes comparing numerical inputs to predefined rules, constraints, or mathematical calculations to ensure data integrity and correctness. </rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="check_method"/>
    <rdfs:subClassOf rdf:resource="CheckMethod"/>
    <dct:title xml:lang="en">Numerical Check Method</dct:title>
