  </owl:Class>
  <owl:Class rdf:about="ComponentCheckMethod">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Component Check Method</rdfs:label>
    <rdfs:comment xml:lang="en">Refers to a process of inspecting and verifying specific components or elements of a building or infrastructure to ensure they meet the required standards, specifications, and regulations. This method is used to assess the quality, functionality, and safety of individual components used in construction projects. Formally defined as checks for some feature of interest</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="check_method"/>
    <rdfs:subClassOf rdf:resource="CheckMethod"/>
    <owl:intersectionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="CheckMethod"/>
      <owl:Restriction rdf:nodeID="n7daa7dbb2c72452aaceb86aedabb4ad3b1">
        <owl:allValuesFrom>
          <owl:Restriction rdf:nodeID="n7daa7dbb2c72452aaceb86aedabb4ad3b2">
            <owl:allValuesFrom rdf:resource="FeatureOfInterest"/>
            <owl:onProperty>
              <rdf:Description rdf:nodeID="n7daa7dbb2c72452aaceb86aedabb4ad3b3">
                <owl:inverseOf rdf:resource="forDesign"/>
              </rdf:Description>
            </owl:onProperty>
          </owl:Restriction>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="forDesign"/>
      </owl:Restriction>
    </owl:intersectionOf>
    <dct:title xml:lang="en">Component Check Method</dct:title>
    <dct:description xml:lang="en">Refers to a process of inspecting and verifying specific components or elements of a building or infrastructure to ensure they meet the required standards, specifications, and regulations. This method is used to assess the quality, functionality, and safety of individual components used in construction projects. Formally defined as checks for some feature of interest</dct:description>
  </owl:Class>
