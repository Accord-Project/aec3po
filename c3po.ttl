#################################################################
#    Classification Design
#################################################################

c3po:ClassificationDesign a owl:Class ;
  rdfs:label "classification design"@en ;
  rdfs:comment "Designs that take values in classification schemes, described using SKOS"@en ;
  skos:note """classification design are expected to be taken in existing concept schemes.
  C3PO defines some concept schemes such as c3po:building_structure
  Existing concept schemes may also be used such as Ifc classes, or bSDD."""@en ;
  owl:equivalentClass [
    owl:intersectionOf (
     skos:Concept
     [  a owl:Restriction ;
        owl:onProperty skos:inScheme ;
        owl:someValuesFrom skos:ConceptScheme .
     ]
   )
  ] .

#################################################################
#    Property Design
#################################################################

c3po:PropertyDesign a owl:Class ;
  rdfs:subClassOf c3po:Design ;
  rdfs:label "property design"@en ;
  rdfs:comment "Designs that assign a value expressed as a literal to some property of a feature of interest."@en ;
  skos:note "The value of the literal may be a XSD literal (boolean, integer, decimal, string), it may also use custom datatypes such as cdt:ucum"@en ;
  rdfs:subClassOf 
    [ a owl:Restriction ;
      owl:onProperty [ owl:inverseOf c3po:hasDesign ] ;
      owl:allValuesFrom c3po:Property ;
    ] ,
    [  a owl:Restriction ;
      owl:onProperty c3po:hasValue ;
      owl:cardinality 1 .
    ] .
    

c3po:hasValue a owl:DatatypeProperty , owl:FunctionalProperty ;
  rdfs:label "has value"@en ;
  rdfs:comment """links a property design to the value of that property, expressed as a literal."""@en ;
  skos:note """The value of the literal may be a XSD literal (boolean, integer, decimal, string), it may also use custom datatypes such as cdt:ucum"""@en ;
  rdfs:domain c3po:PropertyDesign .


#################################################################
#    Property Kind Design
#################################################################

c3po:PropertyKindDesign a owl:Class ;
  rdfs:subClassOf c3po:Design ;
  rdfs:label "property kind design"@en ;
  rdfs:comment "Designs that constrain the value for some property kind."@en ;
  skos:note """The comparisonOperator must be taken from the c3po:ComparisonOperatorTable concept scheme defined in module c3po:comparisonOperators"""@en ;
  rdfs:subClassOf 
    [  a owl:Restriction ;
      owl:onProperty [ owl:inverseOf c3po:hasDesign ] ;
      owl:allValuesFrom c3po:PropertyKind 
    ] ,
    [  a owl:Restriction ;
      owl:onProperty c3po:hasValue ;
      owl:cardinality 1 
    ] ,
    [  a owl:Restriction ;
      owl:onProperty c3po:hasComparisonOperator ;
      owl:cardinality 1 
    ] ;
  rdfs:seeAlso c3po:comparisonOperators .

c3po:hasComparisonOperator a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf c3po:hasDesign ;
  rdfs:range c3po:ComparisonOperator ;
  rdfs:label "has comparisonOperator"@en ;
  rdfs:comment """The comparisonOperator of a property kind design;
  A set of values is defined by C3PO in the corresponding concept scheme."""@en .



c3po:ComparisonOperator a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "comparisonComparisonOperator"@en ;
  rdfs:comment "Formally defined as the set of skos:Concept in concept scheme c3po:ComparisonOperatorTable"@en ;
  owl:equivalentClass [
    owl:intersectionOf (
     skos:Concept
     [  a owl:Restriction ;
        owl:onProperty skos:inScheme ;
        owl:hasValue eli:ComparisonOperatorTable .
     ]
   )
  ] .

c3po:ComparisonOperatorTable a skos:ConceptScheme ;
  skos:hasTopConcept c3po:ComparisonOperator-lt, c3po:ComparisonOperator-le, c3po:ComparisonOperator-eq, c3po:ComparisonOperator-ge, c3po:ComparisonOperator-gt ;
  rdfs:label "Opeator table"@en ;
  skos:definition """A set of values for the comparisonComparisonOperator.
  C3PO includes a set of values for this table.
  These values are :
  - lt : lower than;
  - le : lower or equal;
  - eq : equals;
  - ge : greater or equal;
  - gt : greater than."""@en .

c3po:ComparisonOperator-lt a c3po:ComparisonOperator, skos:Concept ;
  skos:inScheme c3po:ComparisonOperatorTable ;
  skos:topConceptOf c3po:ComparisonOperatorTable ;
  skos:definition "Less than comparisonComparisonOperator."@en ;
  skos:prefLabel "less than"@en .

c3po:ComparisonOperator-le a c3po:ComparisonOperator, skos:Concept ;
  skos:inScheme c3po:ComparisonOperatorTable ;
  skos:topConceptOf c3po:ComparisonOperatorTable ;
  skos:definition "Less or equal comparisonComparisonOperator."@en ;
  skos:prefLabel "less or equal"@en .

c3po:ComparisonOperator-eq a c3po:ComparisonOperator, skos:Concept ;
  skos:inScheme c3po:ComparisonOperatorTable ;
  skos:topConceptOf c3po:ComparisonOperatorTable ;
  skos:definition "Equals comparisonComparisonOperator."@en ;
  skos:prefLabel "equals"@en .

c3po:ComparisonOperator-ge a c3po:ComparisonOperator, skos:Concept ;
  skos:inScheme c3po:ComparisonOperatorTable ;
  skos:topConceptOf c3po:ComparisonOperatorTable ;
  skos:definition "Greater or equal comparisonComparisonOperator."@en ;
  skos:prefLabel "greater or equal"@en .
  
c3po:ComparisonOperator-gt a c3po:ComparisonOperator, skos:Concept ;
  skos:inScheme c3po:ComparisonOperatorTable ;
  skos:topConceptOf c3po:ComparisonOperatorTable ;
  skos:definition "Greater than comparisonComparisonOperator."@en ;
  skos:prefLabel "greater than"@en .



#################################################################
#     Checks as operationalization of document subdivisions 
#################################################################


c3po:Check a owl:Class ;
    rdfs:subClassOf dul:Method ;
    rdfs:label "Check"@en ;
    rdfs:comment """A piece of information that operationalizes a clause, usually executed in order to control the conformance of some entity against a design. A Check is re-usable, and may be executed several times."""@en ;
    skos:note """A Check may be linked to some Design through the `forDesign` property.
    A Check may be linked to some Property Kind through the `forPropertyKind` property."""@en .

c3po:isOperationalizedBy a owl:ObjectProperty ;
  rdfs:domain eli:DocumentSubdivision ; # encompasses c3po:Statement
  rdfs:range c3po:Check ;
  owl:inverseOf c3po:operationalizes .

c3po:operationalizes a owl:ObjectProperty ;
  rdfs:domain c3po:Check ;
  rdfs:range eli:DocumentSubdivision ;
  owl:inverseOf c3po:isOperationalizedBy .

c3po:hasSubCheck a owl:ObjectProperty ;
  rdfs:label "has sub check"@en ;
  rdfs:comment """Links a AndCheck to the set of checks that all need to be verified.
Links a OrCheck to the set of checks that at least one must verify."""@en ;
  rdfs:domain c3po:Check ;
  rdfs:range c3po:Check .
 


c3po:ComponentCheck  a owl:Class ;
  rdfs:subClassOf c3po:Check ;
  rdfs:label "Component Check"@en ;
  rdfs:comment """Formally defined as checks for some feature of interest"""@en ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
      c3po:Check 
      [
        a owl:Restriction ; 
        owl:onProperty c3po:forDesign ;
        owl:allValuesFrom [
            a owl:Restriction ;
            owl:onProperty [ owl:inverseOf c3po:forDesign ] ;
            owl:allValuesFrom c3po:FeatureOfInterest
        ]
      ]
    )
  ] .

c3po:DeclarativeCheck a owl:Class ;
    rdfs:subClassOf c3po:Check ;
    rdfs:label "Declarative Check"@en ;
    rdfs:comment """Declarative check, expressed in IDS, SPARQL, BimSPARQL (SPARQL with eg HDF5 for geometry access, with extra SPARQL functions), or any other declarative language. 

The identifier of a Declarative Check is expected to be dereferenceable, potentially with mediatype-based content negotiation. The content type of the retrieved representation indicates which type of declaration language it uses. Alternatively, the check expression may be given using a property such as c3po:asText."""@en .

c3po:IDSCheck a owl:Class ;
    rdfs:subClassOf c3po:DeclarativeCheck ;
    skos:note "must be refined" .

c3po:ProceduralCheck a owl:Class ;
    rdfs:subClassOf c3po:Check ;
    rdfs:label "Procedural Check"@en ;
    rdfs:comment """Procedural checks are semantically described but opaque modules, implemented in any language or as REST API, invoked by micro-services.
Alternatively, the check procedure may be given using a property such as c3po:asText."""@en .


#################################################################
#     Execution of checks
#################################################################

c3po:Checking a owl:Class ;
    rdfs:subClassOf dul:Event ;
    rdfs:label "Checking"@en ;
    rdfs:comment """Act of verifying a check to determine the conformance of some design. 
    Links to Verifier that made the checking act ;
    Links to the checks that were verified, either satified, or unsatisfied;
    Links to the designs that were satisfied, or unsatisfied."""@en ;
    rdfs:subClassOf [ 
      a owl:Restriction ;
      owl:onProperty c3po:madeBy ;
      owl:someValuesFrom c3po:Verifier
    ] , [ 
      a owl:Restriction ;
      owl:onProperty c3po:checked ;
      owl:minCardinality 1
    ] .

c3po:madeBy a owl:ObjectProperty ;
  rdfs:label "made by"@en ;
  rdfs:comment """Links a checking act to the verifier that made it."""@en ;
  rdfs:domain c3po:Checking ;
  rdfs:range c3po:Verifier .

c3po:checked a owl:ObjectProperty ;
  rdfs:label "checked"@en ;
  rdfs:comment """Links a checking act to the feature of interest, property, design, check, or legal resource, it verified."""@en ;
  rdfs:domain c3po:Checking ;
  rdfs:range c3po:Check .

c3po:satisfied a owl:ObjectProperty ;
  rdfs:subPropertyOf c3po:checked ;
  rdfs:label "satisfied"@en ;
  rdfs:comment """Links a checking act to a satisfied feature of interest, property, design, check, or legal resource."""@en ;
  rdfs:domain c3po:Checking ;
  rdfs:range [ 
    a owl:Class ; 
    owl:unionOf ( 
      c3po:FeatureOfInterest 
      c3po:Property 
      c3po:Design 
      c3po:Check
      eli:LegalResource 
      )
    ] .

c3po:unsatisfied a owl:ObjectProperty ;
  rdfs:subPropertyOf c3po:checked ;
  rdfs:label "unsatisfied check"@en ;
  rdfs:comment """Links a checking act to an unsatisfied  feature of interest, property, design, check, or legal resource."""@en .

c3po:hasEvidence a owl:ObjectProperty ;
    rdfs:domain c3po:Checking ;
    rdfs:label "has evidence"@en ;
    rdfs:comment """Links a checking act to some evidence that demonstrates that this checking act is satisfied or unsatisfied."""@en .

#################################################################
#     Verifier 
#################################################################

c3po:LegalVerifier a owl:Class ;
 rdfs:label "Legal Verifier"@en ;
  rdfs:comment """ """@en .

c3po:StateVerifier a owl:Clas ;
	rdfs:subClassOf c3po:LegalVerifier.
 	rdfs:label "State Verifier"@en ;
	rdfs:comment """ """@en .

c3po:PrivateVerifier a owl:Clas ;
	rdfs:subClassOf c3po:LegalVerifier.
 	rdfs:label "Provate Verifier"@en ;
	rdfs:comment """ """@en .


#################################################################
#     General axioms
#################################################################


[ a owl:AllDisjointClasses ;
  owl:members ( c3po:FeatureOfInterest
    c3po:Property
    c3po:PropertyKind
    c3po:Design
    eli:LegalResource
    c3po:Check
    c3po:Verifier
    c3po:Checking
    )
] .

