@prefix eli: <http://data.europa.eu/eli/ontology#> .
@prefix rdac: <http://www.rdaregistry.info/Elements/c/> .
@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@prefix prov: <http://www.w3.org/ns/prov#>.

c3po:core a owl:Ontology ;
    dct:title "Construction Compliance Checking and Permitting Ontology"@en ;
    dct:description  "An ontology for Construction Compliance Checking and Permitting, based on the statements on building compliance-related document and design codes."@en ;
    skos:note """Main classes and properties in the C3PO ontology are aligned to higher level ontologies, such as DUL."""@en ;
    dct:modified "2023-05-14"^^xsd:date ;
    dct:contributor <edlira> ; 
    dct:contributor <pan> ;
    dct:contributor <http://maxime-lefrancois.info/me#> ; 
    dct:contributor <thomas> ;
	 dct:contributor <etc> ;
    cc:license <https://creativecommons.org/licenses/by/4.0/> ;
    vann:preferredNamespacePrefix "c3po" ;
    vann:preferredNamespaceUri <> .

skos:Concept a owl:Class .
skos:inScheme a owl:ObjectProperty .
skos:ConceptScheme a owl:Class .

#################################################################
#    Document
#################################################################
c3po:Document a owl:Class ;
  rdfs:label "Document"@en ;
  rdfs:comment """A document representing AEC regulations, guidelines, building codes etc."""@en ;
  skos:example """Approved Document A, Eurocode, Finish Regulations..."""@en ;

c3po:DocumentSubdivision a owl:Class
rdfs:subClassOf c3po:Document ;
rdfs:label "Document Subdivision"@en ;
skos:example """Paragraph, section, definition..."""@en

dct:hasPart a owl:ObjectProperty ;
  rdfs:label "has Part"@en ;
  rdfs:comment """A related resource that is included either physically or logically in the described resource."""@en ;
  skos:example """A document has as part of it paragraphs, sections, RASE tags etc. """@en ;
  rdfs:domain c3po:Document ;
  rdfs:range c3po:DocumentSubdivision ;
  owl:inverseOf dct:isPartOf .
  
dct:identifier a owl:ObjectProperty ;
  rdfs:label "Identifier"@en ;
  rdfs:comment """An unambiguous reference to the resource within a given context. """@en ;
  skos:example """To identify the document or a section within the document. Examples include International Standard Book Number (ISBN), Digital Object Identifier (DOI), and Uniform Resource Name (URN). Persistent identifiers should be provided as HTTP URIs. """@en ;
  rdfs:domain [ a owl:Class ; owl:unionOf ( c3po:Document c3po:DocumentSubdivision c3po:Statement ) ] ;
  rdfs:range rdfs:Literal .
  
dct:references a owl:ObjectProperty ;
  rdfs:label "References"@en ;
  rdfs:comment """A related resource that is referenced, cited, or otherwise pointed to by the described resource."""@en ;
  skos:example """Ensure that the measured ceiling height is at least 2.4 meters (7.9 feet) in all habitable rooms. Refer to the Building Code Standards, Section 3.2.1, to verify the requirement for minimum ceiling heights. """@en ;
  rdfs:domain c3po:Document ;
  rdfs:range rdfs:Literal ;
  owl:inverseOf dct:isReferencedBy .

#################################################################
#    Statement
#################################################################
c3po:Statement a owl:Class ;
rdfs:subClassOf c3po:DocumentSubdivision ;
  rdfs:label "Statement"@en ;
  rdfs:comment """Something said in a building compliance-related document"""@en ;
  skos:example """Any  solar protection  solutions  in  the  building  shall  be  taken  into  account  in  the  calculation  of  solar energy gains for the building."""@en ;

dct:source a owl:ObjectProperty ;
  rdfs:label "Source"@en ;
  rdfs:comment """A related resource from which the described statement is derived. """@en ;
  skos:example """Recommended best practice is to identify the related resource by means of a string conforming to a formal identification system."""@en ;
  rdfs:domain c3po:Statement ;
  rdfs:range rdfs:Literal .

c3po:asText a owl:DatatypeProperty ;
  rdfs:label "as text"@en ;
  rdfs:comment """The text as written in a compliance related document or the content of a declarative or procedural check, that verifiers can use."""@en ;
  rdfs:domain [ a owl:Class ; owl:unionOf ( c3po:DeclarativeCheck c3po:ProceduralCheck c3po:Statement) ] .



#################################################################
#    Statement type
#################################################################
c3po:CheckStatement rdf:type owl:Class ;
            rdfs:subClassOf c3po:Statement .

c3po:CategoryCheckStatement rdf:type owl:Class ;
                    rdfs:subClassOf c3po:CheckCheckStatement .


c3po:CertificateCheckStatement rdf:type owl:Class ;
                       rdfs:subClassOf c3po:CheckCheckStatement .


c3po:ChecklistStatement rdf:type owl:Class ;
                         rdfs:subClassOf c3po:Statement .
						 
c3po:DefinitionStatement rdf:type owl:Class ;
    rdfs:subClassOf c3po:Statement .

c3po:HumanEvaluatedStatement rdf:type owl:Class ;
    rdfs:subClassOf c3po:Statement . 
rdfs: comment"""A type of check that is evaluated by a human. Those are primarily for issues of perception, e.g. "is aesthetically appropriate".""" .	
 
c3po:AndCheck a owl:Class ;
    rdfs:label "And Check"@en ;
    rdfs:comment """Check verified if and only if all of its sub-checks are verified."""@en ;
    rdfs:subClassOf c3po:ChecklistStatement ;
    rdfs:subClassOf [ a owl:Restriction ; owl:onProperty c3po:hasSubCheck ; owl:minCardinality 1 ] .
    
c3po:OrCheck a owl:Class ;
    rdfs:label "Or Check"@en ;
    rdfs:comment """Check verified if and only if at least one of its sub-checks is verified."""@en ;
    rdfs:subClassOf c3po:ChecklistStatement ;
    rdfs:subClassOf [ a owl:Restriction ; owl:onProperty c3po:hasSubCheck ; owl:minCardinality 1 ] .

c3po:NumericalCheck rdf:type owl:Class ;
                     rdfs:subClassOf c3po:Check ;
                     rdfs:comment "A check that involves checking one numerical property against another, i.e. x < y or similar." .

#################################################################
#    Features of Interest
#################################################################

c3po:FeatureOfInterest a owl:Class ;
  rdfs:subClassOf dul:Object ;
  rdfs:label "Feature Of Interest"@en ;
  rdfs:comment """The object whose conformance against checks is verified."""@en ;
  skos:example """A building, a building space, a building element, the set of columns in a building, ..."""@en ;
  skos:note """It is expected that classes in other ontologies are used to multi-type instances of FeatureOfInterest.
  For example a feature of interest may also be:
  - a bot:Zone or a bot:Element
  - a ifc:IfcRamp, a ifc:IfcSurfaceFeature"""@en .


#################################################################
#    Properties of features of interest
#################################################################

c3po:Property a owl:Class ;
  rdfs:subClassOf dul:Quality ;
  rdfs:label "Property"@en ;
  rdfs:comment """An aspect of a feature of interest that is intrinsic to and cannot exist without the feature of interest."""@en ;
  skos:example """The height of a building, the area of a space, the U-value of a wall"""@en .

c3po:hasProperty a owl:ObjectProperty ;
  rdfs:label "has Property"@en ;
  rdfs:comment """Links a feature of interest to one of its aspect that is intrinsic to and cannot exist without it."""@en ;
  skos:example """Links a space to the area property of that space, a wall to the U-value property of that wall..."""@en ;
  rdfs:domain c3po:FeatureOfInterest ;
  rdfs:range c3po:Property .


#################################################################
#    Kinds of properties
#################################################################

c3po:PropertyKind a owl:Class ;
  rdfs:subClassOf dul:Region ;
  rdfs:label "Property Kind"@en ;
  rdfs:comment """Any region in a dimensional space, which can be used as a value for a quality of an entity.
  It is expected that the property kinds are organized in a hierarchy of concepts using skos:broader/skos:narrower.
  Property kinds may be taken from the QUDT Quantity Kind vocabulary http://qudt.org/2.1/vocab/quantitykind  """@en ;
  skos:example """Length, Area, U-Value."""@en ;
  skos:note """Both PropertyKind and QuantityValue are aligned to dul:Region. A dimensional space such as "Length" or "Thermal Transmittance" is a maximal Region, and a specific quantity value such as "13 m" or "3.5 W/(m^2.K)" is a minimal Region."""@en .

c3po:hasPropertyKind a owl:ObjectProperty ;
    rdfs:label "has property kind"@en ;
    rdfs:comment """Links a property of a feature of interest to its kind"""@en ;
    skos:example """A space area property to the property kind "area"."""@en ;
    rdfs:domain c3po:Property ;
    rdfs:range c3po:PropertyKind .

c3po:forPropertyKind a owl:ObjectProperty ;
    rdfs:label "for property kind"@en ;
    rdfs:comment """Links any entity (check, verification, procedure execution) to some property kind it is """@en ;
    rdfs:range c3po:PropertyKind .

# some examples

c3po:CompressiveForce a c3po:PropertyKind , skos:Concept ;
  skos:broader quantitykind:Force .
c3po:CrossSection a c3po:PropertyKind, skos:Concept ;
  skos:broader quantitykind:CrossSection .
c3po:GammaM  a c3po:PropertyKind, skos:Concept .
c3po:SystemStrength a c3po:PropertyKind, skos:Concept .
c3po:hasRequiredfc0k a c3po:PropertyKind, skos:Concept .
c3po:kmod a c3po:PropertyKind, skos:Concept .



#################################################################
#   RASE (Requirement, Applicability, Selection, Exclusion)
#################################################################




#################################################################
#    Design
#################################################################

c3po:Design a owl:Class ;
    rdfs:subClassOf dul:Design ;
    rdfs:label "Design"@en ;
    rdfs:comment """A Description of some design of a feature of interest, in terms of structure, geometry, and function. 
    May be linked to a feature of interest to describe the design it has, for example using the ClassificationDesign subclass.
    May be linked to a check, verifier, or check result, to describe the kind of design it is for."""@en ;
    skos:example """Example: 
- the type of use of a building (Residential / Commercial)
- the type of structural material (steel/ RC / timber)
- the designed height of a building (High-rise, Mid-rise, Low-rise)
- the area of space abc in IFC model xyz is 14 m2"""@en .

c3po:hasDesign a owl:ObjectProperty ;
    rdfs:label "has design"@en ;
    rdfs:comment """Links a feature of interest or a property to a description of its design."""@en ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( c3po:FeatureOfInterest c3po:Property ) ] ;
    rdfs:range c3po:Design .

