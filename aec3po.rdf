  <owl:DatatypeProperty rdf:about="hasValue">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:label xml:lang="en">has value</rdfs:label>
    <rdfs:comment xml:lang="en">de facto used to link something (anything: property, property design, check, cell, column, ...) to a value, expressed as a literal.</rdfs:comment>
    <rdfs:isDefinedBy>
