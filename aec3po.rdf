        <owl:imports rdf:resource="statement"/>
        <owl:imports rdf:resource="table"/>
        <owl:imports rdf:resource="evidence"/>
        <owl:imports rdf:resource="data_requirement"/>
        <owl:imports rdf:resource="rase_statement"/>
        <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-05-24</dct:created>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-02-28</dct:modified>
      </owl:Ontology>
    </rdfs:isDefinedBy>
    <skos:note xml:lang="en">The value of the literal may be a XSD literal (boolean, integer, decimal, string), it may also use custom datatypes such as cdt:ucum</skos:note>
    <rdfs:domain>
      <owl:Class rdf:nodeID="n735d9e47e195419d9117ac3a67f531e2b1">
