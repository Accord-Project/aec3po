        <dct:description xml:lang="en">The Architecture, Engineering and Construction Compliance Checking and Permitting ontology (AEC3PO), based on the statements on building compliance-related document and design codes.</dct:description>
        <dct:title xml:lang="en">AEC3PO</dct:title>
        <vann:preferredNamespacePrefix>aec3po</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/lbd/aec3po/</vann:preferredNamespaceUri>
        <skos:note xml:lang="en">Main classes and properties in the AEC3PO ontology are aligned to higher level ontologies, such as DUL.</skos:note>
        <owl:imports rdf:resource="check_method"/>
        <owl:imports rdf:resource="checking_act"/>
        <owl:imports rdf:resource="conformance_report"/>
        <owl:imports rdf:resource="design"/>
