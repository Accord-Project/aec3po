<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xml:base="https://w3id.org/lbd/aec3po/"
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
