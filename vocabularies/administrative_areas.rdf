  <owl:ObjectProperty rdf:about="forAdministrativeArea">
    <rdfs:label xml:lang="en">for administrative area</rdfs:label>
    <rdfs:comment xml:lang="en">The administrative area for which something applies.
  A set of values is defined by aec3po in the corresponding concept scheme.</rdfs:comment>
    <rdfs:isDefinedBy>
      <owl:Ontology rdf:about="administrative_areas">
        <cc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <dct:contributor rdf:resource="http://maxime-lefrancois.info/me#"/>
        <dct:description xml:lang="en">Definition of an administrative areas concept scheme for AEC3PO.</dct:description>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-05-25</dct:modified>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-08-31</dct:modified>
        <dct:title xml:lang="en">AEC3PO: Administrative Areas</dct:title>
        <vann:preferredNamespacePrefix>aec3po</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/lbd/aec3po/</vann:preferredNamespaceUri>
        <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-05-25</dct:created>
      </owl:Ontology>
    </rdfs:isDefinedBy>
    <rdfs:range rdf:resource="AdministrativeArea"/>
  </owl:ObjectProperty>
  <owl:NamedIndividual rdf:about="England">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="AdministrativeArea"/>
    <rdfs:isDefinedBy rdf:resource="administrative_areas"/>
    <skos:definition xml:lang="en">England</skos:definition>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="AdministrativeAreaNomenclature">
        <rdfs:label xml:lang="en">Administrative Area nomenclature</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="administrative_areas"/>
        <skos:definition xml:lang="en">A set of values for the administrative areas.
  aec3po includes a set of values for this nomenclature.
  These values are :
  - England
  - Estonia
  - Finland
  - France
  - Germany
  - Italy
  - Spain</skos:definition>
        <skos:hasTopConcept rdf:resource="England"/>
        <skos:hasTopConcept rdf:resource="Estonia"/>
        <skos:hasTopConcept rdf:resource="Finland"/>
        <skos:hasTopConcept rdf:resource="France"/>
        <skos:hasTopConcept rdf:resource="Germany"/>
        <skos:hasTopConcept rdf:resource="Italy"/>
        <skos:hasTopConcept rdf:resource="Spain"/>
      </skos:ConceptScheme>
    </skos:inScheme>
    <skos:prefLabel xml:lang="en">England</skos:prefLabel>
    <skos:topConceptOf rdf:resource="AdministrativeAreaNomenclature"/>
  </owl:NamedIndividual>
