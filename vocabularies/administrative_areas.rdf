    <rdfs:seeAlso rdf:resource="https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/atu"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <owl:equivalentClass rdf:resource="http://data.europa.eu/eli/ontology#AdministrativeArea"/>
