    rdfs:isDefinedBy <administrative_areas> ;
    skos:definition "Estonia"@en ;
    skos:inScheme <AdministrativeAreaNomenclature> ;
    skos:prefLabel "Estonia"@en ;
    skos:topConceptOf <AdministrativeAreaNomenclature> .

