  <skos:ConceptScheme rdf:about="PermittingStageNomenclature">
    <rdfs:label xml:lang="en">Permitting stage nomenclature</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="permitting_stage"/>
    <skos:definition xml:lang="en">A set of values for permitting stages.
  AEC3PO includes a set of values for this nomenclature.
  These values are :
  - UK_Building_Control_Submission;
  - UK_Planning_Permission.</skos:definition>
    <skos:hasTopConcept>
      <owl:NamedIndividual rdf:about="UK_Building_Control_Submission">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdf:type rdf:resource="PermittingStage"/>
        <rdfs:isDefinedBy rdf:resource="permitting_stage"/>
        <skos:definition xml:lang="en">The UK Building Control Submission permitting stage.</skos:definition>
        <skos:inScheme rdf:resource="PermittingStageNomenclature"/>
        <skos:prefLabel xml:lang="en">building control submission</skos:prefLabel>
        <skos:topConceptOf rdf:resource="PermittingStageNomenclature"/>
        <aec3po:forAdministrativeArea rdf:resource="England"/>
      </owl:NamedIndividual>
    </skos:hasTopConcept>
    <skos:hasTopConcept>
      <owl:NamedIndividual rdf:about="UK_Planning_Permission">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdf:type rdf:resource="PermittingStage"/>
        <rdfs:isDefinedBy rdf:resource="permitting_stage"/>
        <skos:definition xml:lang="en">The UK Planning Permission permitting stage.</skos:definition>
        <skos:inScheme rdf:resource="PermittingStageNomenclature"/>
