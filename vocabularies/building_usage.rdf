<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xml:base="https://w3id.org/lbd/aec3po/"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:skos="http://www.w3.org/2004/02/skos/core#"
  xmlns:vann="http://purl.org/vocab/vann/"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:aec3po="https://w3id.org/lbd/aec3po/"
  xmlns:cc="http://creativecommons.org/ns#"
>
  <owl:FunctionalProperty rdf:about="hasBuildingUsage">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has building usage</rdfs:label>
    <rdfs:comment xml:lang="en">The building usage design of a feature of interest or a property of that feature of interest.
  A set of values is defined by aec3po in the corresponding concept scheme.</rdfs:comment>
    <rdfs:isDefinedBy>
      <owl:Ontology rdf:about="building_usage">
        <cc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <dct:contributor rdf:resource="http://maxime-lefrancois.info/me#"/>
        <dct:description xml:lang="en">Definition of a building usage concept scheme for AEC3PO.</dct:description>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-04-26</dct:modified>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-08-31</dct:modified>
        <dct:title xml:lang="en">AEC3PO: Building Usage</dct:title>
        <vann:preferredNamespacePrefix>aec3po</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/lbd/aec3po/</vann:preferredNamespaceUri>
        <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-05-25</dct:created>
      </owl:Ontology>
    </rdfs:isDefinedBy>
    <rdfs:range rdf:resource="BuildingUsage"/>
    <rdfs:subPropertyOf rdf:resource="hasDesign"/>
  </owl:FunctionalProperty>
  <owl:NamedIndividual rdf:about="BuildingUsage-cultural">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="BuildingUsage"/>
    <rdfs:isDefinedBy rdf:resource="building_usage"/>
    <skos:definition xml:lang="en">Building has cultural usage.</skos:definition>
    <skos:inScheme>
      <skos:ConceptScheme rdf:about="BuildingUsageNomenclature">
        <rdfs:label xml:lang="en">Building usage nomenclature</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="building_usage"/>
        <skos:definition xml:lang="en">A set of values for the building usage design.
  AEC3PO includes a set of values for this nomenclature.
  These values are :
  - residential : residential building;
  - commercial : commercial building.</skos:definition>
        <skos:hasTopConcept rdf:resource="BuildingUsage-commercial"/>
        <skos:hasTopConcept rdf:resource="BuildingUsage-residential"/>
      </skos:ConceptScheme>
    </skos:inScheme>
    <skos:prefLabel xml:lang="en">cultural</skos:prefLabel>
    <skos:topConceptOf rdf:resource="BuildingUsageNomenclature"/>
  </owl:NamedIndividual>
  <owl:ObjectProperty rdf:about="forBuildingUsage">
    <rdfs:label xml:lang="en">for building usage</rdfs:label>
    <rdfs:comment xml:lang="en">The building usage for which a specific check, verifier, or check result applies.
  A set of values is defined by aec3po in the corresponding concept scheme.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="building_usage"/>
    <rdfs:range rdf:resource="BuildingUsage"/>
    <rdfs:subPropertyOf rdf:resource="forDesign"/>
  </owl:ObjectProperty>
  <owl:NamedIndividual rdf:about="BuildingUsage-residential">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="BuildingUsage"/>
    <rdfs:isDefinedBy rdf:resource="building_usage"/>
    <skos:definition xml:lang="en">Building has residential usage.</skos:definition>
    <skos:inScheme rdf:resource="BuildingUsageNomenclature"/>
    <skos:prefLabel xml:lang="en">residential</skos:prefLabel>
    <skos:topConceptOf rdf:resource="BuildingUsageNomenclature"/>
  </owl:NamedIndividual>
  <owl:Class rdf:about="BuildingUsage">
    <rdfs:label xml:lang="en">building usage</rdfs:label>
    <rdfs:comment xml:lang="en">Formally defined as the set of skos:Concept in concept scheme aec3po:BuildingUsageNomenclature</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="building_usage"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:subClassOf rdf:resource="Design"/>
    <owl:equivalentClass>
      <rdf:Description rdf:nodeID="n3ccd51234adf47feb6274349284e865bb1">
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
          <owl:Restriction rdf:nodeID="n3ccd51234adf47feb6274349284e865bb2">
            <owl:hasValue rdf:resource="BuildingUsageNomenclature"/>
            <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </rdf:Description>
    </owl:equivalentClass>
  </owl:Class>
  <owl:NamedIndividual rdf:about="BuildingUsage-commercial">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="BuildingUsage"/>
    <rdfs:isDefinedBy rdf:resource="building_usage"/>
    <skos:definition xml:lang="en">Building has commercial usage.</skos:definition>
    <skos:inScheme rdf:resource="BuildingUsageNomenclature"/>
    <skos:prefLabel xml:lang="en">commercial</skos:prefLabel>
    <skos:topConceptOf rdf:resource="BuildingUsageNomenclature"/>
  </owl:NamedIndividual>
</rdf:RDF>
