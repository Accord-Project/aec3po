  <skos:ConceptScheme rdf:about="DisciplineNomenclature">
    <rdfs:label xml:lang="en">Discipline nomenclature</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="discipline"/>
    <skos:definition xml:lang="en">A set of values for disciplines.
  AEC3PO includes a set of values for this nomenclature.
  These values are :
  - architecture;
  - construction;
  - structural engineering;
  - building services;
  - planning.</skos:definition>
    <skos:hasTopConcept>
      <owl:NamedIndividual rdf:about="Discipline-architecture">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdf:type rdf:resource="Discipline"/>
        <rdfs:isDefinedBy rdf:resource="discipline"/>
        <skos:definition xml:lang="en">The Architecture discipline.</skos:definition>
        <skos:inScheme rdf:resource="DisciplineNomenclature"/>
        <skos:prefLabel xml:lang="en">architecture</skos:prefLabel>
