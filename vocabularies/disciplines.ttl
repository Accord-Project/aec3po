@prefix aec3po: <https://w3id.org/lbd/aec3po/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

aec3po:architecture a aec3po:Discipline .

aec3po:buildingServices a aec3po:Discipline .

aec3po:construction a aec3po:Discipline .

aec3po:hasDiscipline a owl:ObjectProperty ;
    rdfs:label "has discipline"@en ;
    rdfs:comment "Links an entity (procedure, statement, verifier, ...) to the discipline to which it pertains."@en ;
    rdfs:range aec3po:Discipline .

aec3po:planning a aec3po:Discipline .

aec3po:structuralEngineering a aec3po:Discipline .

aec3po:Discipline a owl:Class ;
    rdfs:label "Discipline"@en ;
    rdfs:comment "The AEC discipline to which this statement pertains." ;
    rdfs:subClassOf skos:Concept .

