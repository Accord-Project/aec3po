  </rdf:Description>
  <owl:Class rdf:about="Drawing">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Drawing</rdfs:label>
    <rdfs:comment xml:lang="en">Drawing is a form of visual art in which an artist uses instruments to mark paper or other two-dimensional surface. (Source: Wikipedia)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="Document"/>
    <rdfs:isDefinedBy rdf:resource="document"/>
    <dct:title xml:lang="en">Drawing</dct:title>
    <dct:description xml:lang="en">Drawing is a form of visual art in which an artist uses instruments to mark paper or other two-dimensional surface. (Source: Wikipedia)</dct:description>
  </owl:Class>
  <owl:Class rdf:about="Image">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Image</rdfs:label>
    <rdfs:comment xml:lang="en">An image is a visual representation of something. It can be two-dimensional, three-dimensional, or somehow otherwise feed into the visual system to convey information. (Source: Wikipedia)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="Document"/>
    <rdfs:isDefinedBy rdf:resource="document"/>
    <dct:title xml:lang="en">Image</dct:title>
    <dct:description xml:lang="en">An image is a visual representation of something. It can be two-dimensional, three-dimensional, or somehow otherwise feed into the visual system to convey information. (Source: Wikipedia)</dct:description>
  </owl:Class>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Restriction">
    <ns1:hasMember rdf:nodeID="nde74e8929339460ea8a8f35a2483d7f5b1"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Class">
