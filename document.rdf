    <ns1:hasMember rdf:resource="Image"/>
    <ns1:hasMember>
      <owl:Class rdf:nodeID="nde74e8929339460ea8a8f35a2483d7f5b2">
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="Document"/>
          <rdf:Description rdf:about="DocumentSubdivision"/>
        </owl:unionOf>
        <ns1:inDomainOf rdf:resource="hasSubdivision"/>
