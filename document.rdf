        </owl:unionOf>
        <ns1:inDomainOf rdf:resource="hasFirstSubdivision"/>
      </owl:Class>
    </ns1:hasMember>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#FunctionalProperty">
