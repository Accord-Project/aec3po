<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xml:base="https://w3id.org/lbd/aec3po/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:ns1="https://w3id.org/profile/ontdoc/"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:prov="http://www.w3.org/ns/prov#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:cc="http://creativecommons.org/ns#"
  xmlns:vann="http://purl.org/vocab/vann/"
  xmlns:dct="http://purl.org/dc/terms/"
>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#ObjectProperty">
    <ns1:hasMember>
      <owl:ObjectProperty rdf:about="location">
        <rdfs:label xml:lang="en">location</rdfs:label>
        <rdfs:comment xml:lang="en">The physical or geographic location of the building or structure represented in the BIM model.</rdfs:comment>
        <rdfs:domain rdf:resource="https://schema.org/Model"/>
        <rdfs:range rdf:resource="https://schema.org/Place"/>
        <owl:equivalentProperty rdf:resource="https://schema.org/spatial"/>
        <dct:title xml:lang="en">location</dct:title>
        <dct:description xml:lang="en">The physical or geographic location of the building or structure represented in the BIM model.</dct:description>
      </owl:ObjectProperty>
    </ns1:hasMember>
    <ns1:hasMember>
      <owl:ObjectProperty rdf:about="locationCoverage">
        <rdfs:label xml:lang="en">location coverage</rdfs:label>
        <rdfs:comment xml:lang="en">The spatialCoverage indicates the place(s) or the administrative areas which are the focus of the BIM model.</rdfs:comment>
        <rdfs:domain rdf:resource="https://schema.org/Model"/>
        <rdfs:range rdf:nodeID="n5a0a36a427c049db807bdfe4863e3dfcb1"/>
        <owl:equivalentProperty rdf:resource="https://schema.org/spatialCoverage"/>
        <dct:title xml:lang="en">location coverage</dct:title>
        <dct:description xml:lang="en">The spatialCoverage indicates the place(s) or the administrative areas which are the focus of the BIM model.</dct:description>
      </owl:ObjectProperty>
    </ns1:hasMember>
    <ns1:hasMember>
      <owl:ObjectProperty rdf:about="hasBuildingPhase">
        <rdfs:label xml:lang="en">has building phase</rdfs:label>
        <rdfs:comment xml:lang="en">describes the relationship between a construction-related entity and the phase of the building process it is associated with. (Inspired by the BIM shared ontology)</rdfs:comment>
        <rdfs:domain rdf:resource="Model"/>
        <rdfs:range rdf:resource="Phase"/>
        <dct:title xml:lang="en">has building phase</dct:title>
        <dct:description xml:lang="en">describes the relationship between a construction-related entity and the phase of the building process it is associated with. (Inspired by the BIM shared ontology)</dct:description>
      </owl:ObjectProperty>
    </ns1:hasMember>
    <ns1:hasMember>
      <owl:ObjectProperty rdf:about="hasElementPhase">
        <rdfs:label xml:lang="en">has element phase</rdfs:label>
        <rdfs:comment xml:lang="en">is used to establish a relationship between a construction element or component and the specific phase of the building process in which that element is relevant or applicable. (Inspired by the BIM shared ontology)</rdfs:comment>
        <rdfs:domain rdf:resource="Model"/>
        <rdfs:range rdf:resource="Phase"/>
        <dct:title xml:lang="en">has element phase</dct:title>
        <dct:description xml:lang="en">is used to establish a relationship between a construction element or component and the specific phase of the building process in which that element is relevant or applicable. (Inspired by the BIM shared ontology)</dct:description>
      </owl:ObjectProperty>
    </ns1:hasMember>
    <ns1:hasMember>
      <owl:ObjectProperty rdf:about="hasDimension">
        <rdfs:label xml:lang="en">has dimension</rdfs:label>
        <rdfs:comment xml:lang="en">is used to represent the physical dimensions or measurements of an object, entity, or element within the BIM model. </rdfs:comment>
        <rdfs:domain rdf:resource="Model"/>
        <rdfs:range rdf:resource="Dimension"/>
        <dct:title xml:lang="en">has dimension</dct:title>
        <dct:description xml:lang="en">is used to represent the physical dimensions or measurements of an object, entity, or element within the BIM model. </dct:description>
      </owl:ObjectProperty>
    </ns1:hasMember>
    <ns1:hasMember>
      <owl:ObjectProperty rdf:about="hasClassification">
        <rdfs:label xml:lang="en">has dimension</rdfs:label>
        <rdfs:comment xml:lang="en">is used to link building components, elements, or other entities to a classification scheme that provides a standardised way of categorising and organising them.. </rdfs:comment>
        <rdfs:domain rdf:resource="Model"/>
        <rdfs:range rdf:resource="Classification"/>
        <dct:title xml:lang="en">has dimension</dct:title>
        <dct:description xml:lang="en">is used to link building components, elements, or other entities to a classification scheme that provides a standardised way of categorising and organising them.. </dct:description>
      </owl:ObjectProperty>
    </ns1:hasMember>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Ontology">
    <ns1:hasMember>
      <owl:Ontology rdf:about="model">
        <cc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <dct:contributor rdf:resource="amna"/>
        <dct:description xml:lang="en">The AEC3PO model module describes a model representing part or the entirety of a site, building, or piece of infrastructure. Typically, this will be in the form of a Building Information Model.</dct:description>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-08-15</dct:modified>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-04-22</dct:modified>
        <dct:title xml:lang="en">AEC3PO:Model</dct:title>
        <owl:imports rdf:resource="evidence"/>
        <vann:preferredNamespacePrefix>aec3po</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/lbd/aec3po/</vann:preferredNamespaceUri>
        <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-08-16</dct:created>
      </owl:Ontology>
    </ns1:hasMember>
  </rdf:Description>
  <owl:Class rdf:about="Classification">
    <rdf:type>
      <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#Class">
        <ns1:hasMember rdf:resource="Model"/>
        <ns1:hasMember rdf:nodeID="n5a0a36a427c049db807bdfe4863e3dfcb1"/>
        <ns1:hasMember rdf:resource="Phase"/>
        <ns1:hasMember rdf:resource="Element"/>
        <ns1:hasMember rdf:resource="Dimension"/>
        <ns1:hasMember rdf:resource="Classification"/>
      </rdf:Description>
    </rdf:type>
    <rdfs:label xml:lang="en">Classification</rdfs:label>
    <rdfs:subClassOf rdf:resource="Model"/>
    <rdfs:comment>Classification systems or codes used to categorise and organise building components.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="model"/>
    <dct:title xml:lang="en">Classification</dct:title>
    <dct:description>Classification systems or codes used to categorise and organise building components.</dct:description>
    <ns1:inRangeOf rdf:resource="hasClassification"/>
  </owl:Class>
  <prov:Agent rdf:about="amna"/>
  <owl:Class rdf:about="Element">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Element</rdfs:label>
    <rdfs:subClassOf rdf:resource="Model"/>
    <rdfs:comment>Specific attributes of individual elements, such as doors, windows, walls, etc.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="model"/>
    <dct:title xml:lang="en">Element</dct:title>
    <dct:description>Specific attributes of individual elements, such as doors, windows, walls, etc.</dct:description>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="material">
    <rdfs:label xml:lang="en">material</rdfs:label>
    <rdfs:comment xml:lang="en">The materials used for different parts of the building, such as walls, floors, and roofs.</rdfs:comment>
    <rdfs:domain>
      <rdf:Description rdf:about="https://schema.org/Model">
        <ns1:inDomainOf rdf:resource="location"/>
        <ns1:inDomainOf rdf:resource="locationCoverage"/>
        <ns1:inDomainOf rdf:resource="material"/>
      </rdf:Description>
    </rdfs:domain>
    <rdfs:range>
      <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string">
        <ns1:inRangeOf rdf:resource="material"/>
      </rdf:Description>
    </rdfs:range>
    <owl:equivalentProperty rdf:resource="https://schema.org/material"/>
    <dct:title xml:lang="en">material</dct:title>
    <dct:description xml:lang="en">The materials used for different parts of the building, such as walls, floors, and roofs.</dct:description>
  </owl:DatatypeProperty>
  <owl:Class rdf:about="Model">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Model</rdfs:label>
    <rdfs:comment>A BIM model is a digital representation of a building or infrastructure project that includes both graphical and non-graphical information.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="model"/>
    <dct:title xml:lang="en">Model</dct:title>
    <dct:description>A BIM model is a digital representation of a building or infrastructure project that includes both graphical and non-graphical information.</dct:description>
    <ns1:superClassOf rdf:resource="Phase"/>
    <ns1:superClassOf rdf:resource="Element"/>
    <ns1:superClassOf rdf:resource="Dimension"/>
    <ns1:superClassOf rdf:resource="Classification"/>
    <ns1:inDomainOf rdf:resource="hasBuildingPhase"/>
    <ns1:inDomainOf rdf:resource="hasElementPhase"/>
    <ns1:inDomainOf rdf:resource="hasDimension"/>
    <ns1:inDomainOf rdf:resource="hasClassification"/>
  </owl:Class>
  <owl:Class rdf:about="Phase">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Phase</rdfs:label>
    <rdfs:subClassOf rdf:resource="Model"/>
    <rdfs:comment>A construction or renovation phase the building model may represent. Inspired by the BIM Shared Ontology (https://www.sciencedirect.com/science/article/pii/S0926580517302364)</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="model"/>
    <dct:title xml:lang="en">Phase</dct:title>
    <dct:description>A construction or renovation phase the building model may represent. Inspired by the BIM Shared Ontology (https://www.sciencedirect.com/science/article/pii/S0926580517302364)</dct:description>
    <ns1:inRangeOf rdf:resource="hasBuildingPhase"/>
    <ns1:inRangeOf rdf:resource="hasElementPhase"/>
  </owl:Class>
  <rdf:Description rdf:about="https://schema.org/Place">
    <ns1:inRangeOf rdf:resource="location"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Class">
    <ns1:hasMember rdf:resource="Model"/>
    <ns1:hasMember>
      <owl:Class rdf:nodeID="n5a0a36a427c049db807bdfe4863e3dfcb1">
        <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="https://schema.org/Place"/>
          <rdf:Description rdf:about="AdministrativeArea"/>
        </owl:unionOf>
        <ns1:inRangeOf rdf:resource="locationCoverage"/>
      </owl:Class>
    </ns1:hasMember>
    <ns1:hasMember rdf:resource="Phase"/>
    <ns1:hasMember rdf:resource="Element"/>
    <ns1:hasMember rdf:resource="Dimension"/>
    <ns1:hasMember rdf:resource="Classification"/>
  </rdf:Description>
  <owl:Class rdf:about="Dimension">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Dimension</rdfs:label>
    <rdfs:subClassOf rdf:resource="Model"/>
    <rdfs:comment>Measurements and sizes of various building components and elements.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="model"/>
    <dct:title xml:lang="en">Dimension</dct:title>
    <dct:description>Measurements and sizes of various building components and elements.</dct:description>
    <ns1:inRangeOf rdf:resource="hasDimension"/>
  </owl:Class>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#DatatypeProperty">
    <ns1:hasMember rdf:resource="material"/>
  </rdf:Description>
</rdf:RDF>
