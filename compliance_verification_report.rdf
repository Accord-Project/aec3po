<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xml:base="https://w3id.org/lbd/aec3po/"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns:prov="http://www.w3.org/ns/prov#"
  xmlns:ns1="https://w3id.org/profile/ontdoc/"
  xmlns:vann="http://purl.org/vocab/vann/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:aec3po="https://w3id.org/lbd/aec3po/"
  xmlns:cc="http://creativecommons.org/ns#"
>
  <owl:Ontology rdf:about="compliance_verification_report">
    <cc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
    <dct:contributor>
      <prov:Agent rdf:about="http://maxime-lefrancois.info/me#"/>
    </dct:contributor>
    <dct:description xml:lang="en">The AEC3PO Compliance Verification Report module describes results of some `aec3po:ProcessVerifier` checking some entity via a `aec3po:CheckingAct`. Entities may be validated or repudiated.</dct:description>
    <dct:title xml:lang="en">AEC3PO: Compliance Verification Report</dct:title>
    <owl:imports rdf:resource="check_method"/>
    <owl:imports rdf:resource="compliance_verification_report"/>
    <vann:preferredNamespacePrefix>aec3po</vann:preferredNamespacePrefix>
    <vann:preferredNamespaceUri>https://w3id.org/lbd/aec3po/</vann:preferredNamespaceUri>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-10-30</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-04-22</dct:modified>
  </owl:Ontology>
  <owl:DatatypeProperty rdf:about="conforms">
    <rdfs:label xml:lang="en">conforms</rdfs:label>
    <rdfs:comment xml:lang="en">True if the Checking act confirms the check was validated, and false otherwise.</rdfs:comment>
    <rdfs:domain rdf:resource="ComplianceVerificationReport"/>
    <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
    <rdfs:range>
      <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#boolean">
        <ns1:inRangeOf rdf:resource="conforms"/>
      </rdf:Description>
    </rdfs:range>
    <dct:title xml:lang="en">conforms</dct:title>
    <dct:description xml:lang="en">True if the Checking act confirms the check was validated, and false otherwise.</dct:description>
  </owl:DatatypeProperty>
  <owl:Class rdf:about="ComplianceVerificationReport">
    <rdf:type>
      <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#Class">
        <ns1:hasMember rdf:resource="ComplianceVerificationReport"/>
        <ns1:hasMember rdf:resource="VerificationResult"/>
        <ns1:hasMember rdf:resource="Severity"/>
      </rdf:Description>
    </rdf:type>
    <rdfs:label xml:lang="en">Compliance Verification Report</rdfs:label>
    <rdfs:comment xml:lang="en">Result of a checking act. May be compliant or not.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
    <rdfs:subClassOf>
      <owl:Restriction rdf:nodeID="n2d272a59dee74cbba188bba1f4351354b1">
        <owl:onProperty rdf:resource="conforms"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:cardinality>
        <ns1:superClassOf rdf:resource="ComplianceVerificationReport"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <dct:title xml:lang="en">Compliance Verification Report</dct:title>
    <dct:description xml:lang="en">Result of a checking act. May be compliant or not.</dct:description>
    <ns1:inDomainOf rdf:resource="conforms"/>
    <ns1:inDomainOf>
      <owl:ObjectProperty rdf:about="hasResult">
        <rdfs:label xml:lang="en">has result</rdfs:label>
        <rdfs:comment xml:lang="en">Links a compliance verification report to some verification result.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
        <rdfs:domain rdf:resource="ComplianceVerificationReport"/>
        <rdfs:range rdf:resource="VerificationResult"/>
        <dct:title xml:lang="en">has result</dct:title>
        <dct:description xml:lang="en">Links a compliance verification report to some verification result.</dct:description>
      </owl:ObjectProperty>
    </ns1:inDomainOf>
  </owl:Class>
  <owl:NamedIndividual rdf:about="Warning">
    <rdf:type rdf:resource="Severity"/>
    <rdfs:label xml:lang="en">Warning</rdfs:label>
    <rdfs:comment xml:lang="en">The severity for a warning validation result.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
    <dct:title xml:lang="en">Warning</dct:title>
    <dct:description xml:lang="en">The severity for a warning validation result.</dct:description>
  </owl:NamedIndividual>
  <owl:ObjectProperty rdf:about="verificationFocus">
    <rdfs:label xml:lang="en">verification focus</rdfs:label>
    <rdfs:comment>Each verification result has exactly one value for the property aec3po:verificationFocus that is equal to the focus entity that has caused the result. This is the focus entity that was validated when the verification result was produced.</rdfs:comment>
    <rdfs:domain rdf:resource="VerificationResult"/>
    <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
    <dct:title xml:lang="en">verification focus</dct:title>
    <dct:description>Each verification result has exactly one value for the property aec3po:verificationFocus that is equal to the focus entity that has caused the result. This is the focus entity that was validated when the verification result was produced.</dct:description>
  </owl:ObjectProperty>
  <owl:NamedIndividual rdf:about="Violation">
    <rdf:type rdf:resource="Severity"/>
    <rdfs:label xml:lang="en">Violation</rdfs:label>
    <rdfs:comment xml:lang="en">The severity for a violation validation result.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
    <dct:title xml:lang="en">Violation</dct:title>
    <dct:description xml:lang="en">The severity for a violation validation result.</dct:description>
  </owl:NamedIndividual>
  <owl:DatatypeProperty rdf:about="resultMessage">
    <rdfs:label xml:lang="en">result message</rdfs:label>
    <rdfs:comment>Verification results may have values for the property aec3po:resultMessage, for example to communicate additional textual details to humans. While aec3po:resultMessage may have multiple values, there should not be two values with the same language tag. These values may be produced based on the statements in the AEC3PO graph.</rdfs:comment>
    <rdfs:domain rdf:resource="VerificationResult"/>
    <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
    <dct:title xml:lang="en">result message</dct:title>
    <dct:description>Verification results may have values for the property aec3po:resultMessage, for example to communicate additional textual details to humans. While aec3po:resultMessage may have multiple values, there should not be two values with the same language tag. These values may be produced based on the statements in the AEC3PO graph.</dct:description>
  </owl:DatatypeProperty>
  <owl:Class rdf:about="VerificationResult">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Verification Result</rdfs:label>
    <rdfs:comment xml:lang="en">Individual verification result of a Compliance Verification Report.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
    <rdfs:subClassOf>
      <owl:Restriction rdf:nodeID="n2d272a59dee74cbba188bba1f4351354b2">
        <owl:onProperty rdf:resource="verificationFocus"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:cardinality>
        <ns1:superClassOf rdf:resource="VerificationResult"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction rdf:nodeID="n2d272a59dee74cbba188bba1f4351354b3">
        <owl:onProperty rdf:resource="hasSeverity"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:cardinality>
        <ns1:superClassOf rdf:resource="VerificationResult"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <dct:title xml:lang="en">Verification Result</dct:title>
    <dct:description xml:lang="en">Individual verification result of a Compliance Verification Report.</dct:description>
    <ns1:inDomainOf rdf:resource="verificationFocus"/>
    <ns1:inDomainOf>
      <owl:ObjectProperty rdf:about="verificationStatement">
        <rdfs:label xml:lang="en">verification statement</rdfs:label>
        <rdfs:comment>Statement that was validated when the verification result was produced.</rdfs:comment>
        <rdfs:domain rdf:resource="VerificationResult"/>
        <rdfs:range rdf:resource="CheckStatement"/>
        <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
        <dct:title xml:lang="en">verification statement</dct:title>
        <dct:description>Statement that was validated when the verification result was produced.</dct:description>
      </owl:ObjectProperty>
    </ns1:inDomainOf>
    <ns1:inDomainOf>
      <owl:ObjectProperty rdf:about="verificationPlanZone">
        <rdfs:label xml:lang="en">verification plan zone</rdfs:label>
        <rdfs:comment>Planning (urban regulation) zone where the verificationFocus entity (building or building component) is located, and that defines the regulation (verificationStatement) that was validated when the verification result was produced.</rdfs:comment>
        <rdfs:domain rdf:resource="VerificationResult"/>
        <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
        <dct:title xml:lang="en">verification plan zone</dct:title>
        <dct:description>Planning (urban regulation) zone where the verificationFocus entity (building or building component) is located, and that defines the regulation (verificationStatement) that was validated when the verification result was produced.</dct:description>
      </owl:ObjectProperty>
    </ns1:inDomainOf>
    <ns1:inDomainOf rdf:resource="resultMessage"/>
    <ns1:inDomainOf>
      <owl:ObjectProperty rdf:about="hasSeverity">
        <rdfs:label xml:lang="en">has severity</rdfs:label>
        <rdfs:comment>Each verification result has exactly one value for the property aec3po:resultSeverity, and this value is an IRI. The value is equal to the value of aec3po:hasSeverity of the statement in the AEC3PO graph that caused the result, defaulting to aec3po:Violation if no aec3po:hasSeverity has been specified for the statement.</rdfs:comment>
        <rdfs:domain rdf:resource="VerificationResult"/>
        <rdfs:range rdf:resource="Severity"/>
        <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
        <dct:title xml:lang="en">has severity</dct:title>
        <dct:description>Each verification result has exactly one value for the property aec3po:resultSeverity, and this value is an IRI. The value is equal to the value of aec3po:hasSeverity of the statement in the AEC3PO graph that caused the result, defaulting to aec3po:Violation if no aec3po:hasSeverity has been specified for the statement.</dct:description>
      </owl:ObjectProperty>
    </ns1:inDomainOf>
    <ns1:inRangeOf rdf:resource="hasResult"/>
  </owl:Class>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#NamedIndividual">
    <ns1:hasMember>
      <owl:NamedIndividual rdf:about="Info">
        <rdf:type rdf:resource="Severity"/>
        <rdfs:label xml:lang="en">Info</rdfs:label>
        <rdfs:comment xml:lang="en">The severity for an informational validation result.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
        <dct:title xml:lang="en">Info</dct:title>
        <dct:description xml:lang="en">The severity for an informational validation result.</dct:description>
      </owl:NamedIndividual>
    </ns1:hasMember>
    <ns1:hasMember rdf:resource="Violation"/>
    <ns1:hasMember rdf:resource="Warning"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#ObjectProperty">
    <ns1:hasMember rdf:resource="hasResult"/>
    <ns1:hasMember rdf:resource="verificationFocus"/>
    <ns1:hasMember rdf:resource="verificationStatement"/>
    <ns1:hasMember rdf:resource="verificationPlanZone"/>
    <ns1:hasMember rdf:resource="hasSeverity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Class">
    <ns1:hasMember rdf:resource="ComplianceVerificationReport"/>
    <ns1:hasMember rdf:resource="VerificationResult"/>
    <ns1:hasMember rdf:resource="Severity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#DatatypeProperty">
    <ns1:hasMember rdf:resource="conforms"/>
    <ns1:hasMember rdf:resource="resultMessage"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Restriction">
    <ns1:hasMember rdf:nodeID="n2d272a59dee74cbba188bba1f4351354b1"/>
    <ns1:hasMember rdf:nodeID="n2d272a59dee74cbba188bba1f4351354b2"/>
    <ns1:hasMember rdf:nodeID="n2d272a59dee74cbba188bba1f4351354b3"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Ontology">
    <ns1:hasMember rdf:resource="compliance_verification_report"/>
  </rdf:Description>
  <owl:Class rdf:about="Severity">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Severity</rdfs:label>
    <rdfs:comment xml:lang="en">The class of validation result severity levels, including violation and warning levels.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="compliance_verification_report"/>
    <dct:title xml:lang="en">Severity</dct:title>
    <dct:description xml:lang="en">The class of validation result severity levels, including violation and warning levels.</dct:description>
    <ns1:inRangeOf rdf:resource="hasSeverity"/>
    <ns1:hasMember rdf:resource="Info"/>
    <ns1:hasMember rdf:resource="Violation"/>
    <ns1:hasMember rdf:resource="Warning"/>
  </owl:Class>
  <rdf:Description rdf:about="CheckStatement">
    <ns1:inRangeOf rdf:resource="verificationStatement"/>
  </rdf:Description>
</rdf:RDF>
